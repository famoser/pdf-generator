{
  "name": "famoser/pdf-generator",
  "description": "a clean pdf generator without dependencies",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Florian Moser",
      "email": "git@famoser.ch"
    }
  ],
  "require": {
    "php": ">=8.2",
    "ext-bcmath": "*",
    "ext-gd": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "famoser/agnes": "^4.2",
    "friendsofphp/php-cs-fixer": "^3",
    "mockery/mockery": "^1.2",
    "phpstan/phpstan": "^2.0",
    "phpunit/phpunit": "^9.5.4",
    "rector/rector": "^2"
  },
  "autoload": {
    "psr-4": {
      "Famoser\\PdfGenerator\\": "src/",
      "Famoser\\DocumentGenerator\\": "document-generator/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Famoser\\PdfGenerator\\Tests\\": "tests/"
    }
  },
  "config": {
    "platform": {
      "php": "8.2"
    },
    "sort-packages": true
  }
}
